{
	"name": "badmansan/docker-simple-skeleton",
	"description": "A simple skeleton for easy docker deployment",
	"require": {
		"php": ">=8.1",
		"doctrine/collections": "^1.6",
		"ext-pdo": "*"
	},
	"require-dev": {
		"roave/security-advisories": "dev-latest"
	},
	"config": {
		"process-timeout": 0,
		"sort-packages": true
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Unit\\": "tests/unit"
		}
	}
}
